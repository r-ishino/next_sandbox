{
  "name": "next_sandbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9990",
    "build": "next build && find ./.next/static -name '*.map' -delete",
    "start": "next start -p 80",
    "start:debug": "next start -p 9990",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fmt": "prettier './**/*.{js,ts,tsx,json,yml,yaml,scss}' --ignore-path .gitignore --check",
    "fmt:fix": "prettier './**/*.{js,ts,tsx,json,yml,yaml,scss}' --ignore-path .gitignore --check --write",
    "stylelint": "stylelint scss 'src/**/*.scss'",
    "stylelint:fix": "stylelint --fix scss 'src/**/*.scss'"
  },
  "dependencies": {
    "next": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
